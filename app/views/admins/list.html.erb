<% if !flash[:notice].blank? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<%end%>
<div class ="Admin list"> 
  <h2> Admins</h2>
  <%= link_to("add new Admin", {:action => "new"}, :class=> 'action news') %>
  <table class="listing" summary="Admin list">
    <tr class= "header">
      <th>&nbsp;</th>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>username</th>
      <th>Actions</th>
    </tr>
    <% @Admins.each do |admin| %>
    <tr>
      <td><%=admin.id %></td>
      <td><%=admin.firstName %></td>
      <td><%=admin.lastName %></td>
      <td><%=admin.username %></td>
      <td class= "actions">
        <%=  link_to("Show", {:action => 'show' , :id => admin.id}, :class => 'action show') %>
        <%=  link_to("Edit", {:action => "edit" , :id => admin.id}, :class=>'action edit') %>
        <%=  link_to("Delete", {:action => 'delete' , :id => admin.id}, :class=>'action delete') %>
      </td>
    </tr>
    <% end %>
  </table>
</div>
     