
<%= link_to("<< Back to List", admins_path, :class => 'back-link') %>

<div class="Admin delete">
  <h2>Delete Admin</h2>
  
  <%= form_for(:admin, :url => admin_path(@Admin.id),:html=> {:method => :delete}) do |f| -%>

    <p>Are you sure you want to permanently delete this Admin?</p>
  
    <p class="reference-name"><%= @Admin.firstName %> <%= @Admin.lastName %> </p>
  
    <div class="form-buttons">
      <%= submit_tag("Delete Admin") %>
    </div>
    
  <% end %>
</div>
