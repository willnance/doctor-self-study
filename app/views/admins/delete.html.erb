<head>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<div class="General-container">
  <div class="hero-unit">
  <h2>Delete Admin</h2>
  
  <%= form_for(:admin, :url => admin_path(@Admin.id),:html=> {:method => :delete}) do |f| -%>

    <p>Are you sure you want to permanently delete this Admin?</p>
  
    <p class="reference-name"><%= @Admin.firstName %> <%= @Admin.lastName %> </p>
  

    <%= submit_tag("Delete Admin", :class => 'btn btn-primary btn-large') %>
    <br/><br/>
    <%= link_to("Back to List", admins_path, :class => 'btn btn-primary btn-large') %>

    
  <% end %>
  </div>
</div>
