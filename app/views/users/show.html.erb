<%= link_to("<< Back to List", {:action => 'list'}, :class => 'back-link') %>
<% if !flash[:notice].blank? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
<%end%>
<div class="user show">
  <h2>Show user</h2>

  <table summary="user detail view">
    <tr>
      <th>User First Name</th>
      <td><%= @User.firstName%></td>
    </tr>
    <tr>
      <th>User Last Name</th>
      <td><%= @User.lastName %></td>
    </tr>
    <tr>
      <th>Username</th>
      <td><%= @User.username %></td>
    </tr>
    <tr>
      <th>Password</th>
      <td><%= @User.hashedPassword %></td>
    </tr>
    <tr>
      <th>Year</th>
      <td><%= @User.year %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @User.created_at %></td>
    </tr>
    <tr>
      <th>Last Updated</th>
      <td><%= @User.updated_at %></td>
    </tr>
  </table>
  
</div>

