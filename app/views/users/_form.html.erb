<table summary="User form fields">
      <tr>
        <th>First Name</th>
        <td><%= f.text_field(:firstName) %></td>
      </tr>
      <tr>
        <th>Last Name</th>
        <td><%= f.text_field(:lastName) %></td>
      </tr>
      <tr>
        <th>Username</th>
        <td><%= f.text_field(:username) %></td>
      </tr>
      <tr>
        <th> Password </th>
        <td><%= f.text_field(:password) %></td>
      </tr>
      <tr>
        <th><%=f.label(:rotation)%></th>
        <td><%=f.select(:rotation, 1..10)%></td> 
      </tr>
      <tr>
        <th><%=f.label(:year)%></th>
        <td><%=f.select(:year, 1..10)%></td> 
      </tr>
    </table>